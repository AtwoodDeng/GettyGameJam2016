 <Worksheet ss:Name="scu">
  <Table ss:ExpandedColumnCount="26" ss:ExpandedRowCount="7" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="65" ss:DefaultRowHeight="15">
   <Column ss:Index="25" ss:AutoFitWidth="0" ss:Width="71"/>
   <Row>
    <Cell ss:Index="23"><Data ss:Type="String">Number</Data></Cell>
    <Cell><Data ss:Type="String">Character</Data></Cell>
    <Cell><Data ss:Type="String">Dialog</Data></Cell>
    <Cell><Data ss:Type="String">Dialog2</Data></Cell>
   </Row>
   <Row ss:Height="18">
    <Cell ss:Index="23"><Data ss:Type="Number">0</Data></Cell>
    <Cell><Data ss:Type="String">TRAVELLER</Data></Cell>
    <Cell ss:StyleID="s125"><Data ss:Type="String">Oh careful with that one! This is French royalty right there. </Data></Cell>
   </Row>
   <Row ss:Height="18">
    <Cell ss:Index="23"><Data ss:Type="Number">1</Data></Cell>
    <Cell><Data ss:Type="String">YOU</Data></Cell>
    <Cell ss:StyleID="s120"><Data ss:Type="String"><Font html:Color="#A2BD90"
       xmlns="http://www.w3.org/TR/REC-html40">Tell me more</Font><Font
       html:Color="#33CCCC" xmlns="http://www.w3.org/TR/REC-html40"> … </Font></Data></Cell>
    <Cell ss:StyleID="s126"><Data ss:Type="String">Gossip facts?</Data></Cell>
   </Row>
   <Row ss:Height="18">
    <Cell ss:Index="23"><Data ss:Type="Number">2</Data></Cell>
    <Cell><Data ss:Type="String">TRAVELLER</Data></Cell>
    <Cell ss:StyleID="s120"><Data ss:Type="String">Louis XV, King of France from 1715 to 1774... The so-called Well-Loved King who ironically earned the hatred of his own people after a series of poor decisions.</Data></Cell>
   </Row>
   <Row ss:Height="18">
    <Cell ss:Index="23"><Data ss:Type="Number">3</Data></Cell>
    <Cell><Data ss:Type="String">TRAVELLER</Data></Cell>
    <Cell ss:StyleID="s120"><Data ss:Type="String"><Font html:Color="#A2BD90"
       xmlns="http://www.w3.org/TR/REC-html40">Some claim the seeds of the French Revolution were planted during his reign …</Font><Font
       html:Color="#33CCCC" xmlns="http://www.w3.org/TR/REC-html40"> </Font></Data></Cell>
   </Row>
   <Row ss:Height="18">
    <Cell ss:Index="23"><Data ss:Type="Number">4</Data></Cell>
    <Cell><Data ss:Type="String">TRAVELLER</Data></Cell>
    <Cell ss:StyleID="s126"><Data ss:Type="String">Oh, gossip! Yes! Let me tell you, Louis XV had a longtime mistress, Madame de Pompadour … </Data></Cell>
   </Row>
   <Row ss:Height="18">
    <Cell ss:Index="23"><Data ss:Type="Number">5</Data></Cell>
    <Cell><Data ss:Type="String">TRAVELLER</Data></Cell>
    <Cell ss:StyleID="s126"><Data ss:Type="String">yes, the great patron of architecture and decorative arts! </Data></Cell>
   </Row>
  </Table>
